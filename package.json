{
  "name": "spendyze-frontend",
  "version": "1.0.0",
  "description": "Frontend for Spendyze AI Finance Tracker",
  "scripts": {
    "build": "esbuild index.tsx --bundle --outfile=dist/bundle.js --minify --define:process.env.NODE_ENV='\"production\"'",
    "dev": "esbuild index.tsx --bundle --outfile=dist/bundle.js --servedir=. --define:process.env.NODE_ENV='\"development\"' --port=3000"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "esbuild": "^0.21.3"
  }
}